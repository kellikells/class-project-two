<div class="container">
    <div class="row">
        {{#each services}}
        <div class="col s12 m7">
            <h4 class="header">{{title}}</h4>
            <div class="card horizontal">
                <div class="card-image">
                </div>
                <div class="card-stacked">
                    <div class="card-content">
                        <p>{{description}}</p>
                        <p>{{price}}</p>
                    </div>
                    <div class="card-action">
                        <a href="#">Full Project</a>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>